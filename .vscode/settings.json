{
    "python.linting.mypyEnabled": true,
    "python.linting.flake8Enabled": true,

    "editor.rulers": [99],

    "search.exclude": {
        "# my stuff": true,
        "weedly.db": true,
        "commands": true,
        "temp.py": true,
        "commands.txt": true,
        "test-app.py": true,
        "test-db.py": true,
        "# Byte-compiled / optimized / DLL files": true,
        "**/__pycache__": true,
        "*.py[cod]": true,
        "*$py.class": true,
        "# C extensions": true,
        "*.so": true,
        "# Distribution / packaging": true,
        ".Python": true,
        "build/": true,
        "develop-eggs/": true,
        "dist/": true,
        "downloads/": true,
        "eggs/": true,
        ".eggs/": true,
        "lib/": true,
        "lib64/": true,
        "parts/": true,
        "sdist/": true,
        "var/": true,
        "wheels/": true,
        "pip-wheel-metadata/": true,
        "share/python-wheels/": true,
        "*.egg-info/": true,
        ".installed.cfg": true,
        "*.egg": true,
        "MANIFEST": true,
        "# PyInstaller": true,
        "#  Usually these files are written by a python script from a template": true,
        "#  before PyInstaller builds the exe, so as to inject date/other infos into it.": true,
        "*.manifest": true,
        "*.spec": true,
        "# Installer logs": true,
        "pip-log.txt": true,
        "pip-delete-this-directory.txt": true,
        "# Unit test / coverage reports": true,
        "htmlcov/": true,
        ".tox/": true,
        ".nox/": true,
        ".coverage": true,
        ".coverage.*": true,
        ".cache": true,
        "nosetests.xml": true,
        "coverage.xml": true,
        "*.cover": true,
        ".hypothesis/": true,
        ".pytest_cache/": true,
        "# Translations": true,
        "*.mo": true,
        "*.pot": true,
        "# Django stuff:": true,
        "*.log": true,
        "local_settings.py": true,
        "db.sqlite3": true,
        "# Flask stuff:": true,
        "instance/": true,
        ".webassets-cache": true,
        "# Scrapy stuff:": true,
        ".scrapy": true,
        "# Sphinx documentation": true,
        "docs/_build/": true,
        "# PyBuilder": true,
        "target/": true,
        "# Jupyter Notebook": true,
        ".ipynb_checkpoints": true,
        "# IPython": true,
        "profile_default/": true,
        "ipython_config.py": true,
        "# pyenv": true,
        "# .python-version  # do not ignore for service projects": true,
        "# pipenv": true,
        "#   According to pypa/pipenv#598, it is recommended to include Pipfile.lock in version control.": true,
        "#   However, in case of collaboration, if having platform-specific dependencies or dependencies": true,
        "#   having no cross-platform support, pipenv may install dependencies that don't work, or not": true,
        "#   install all needed dependencies.": true,
        "#Pipfile.lock": true,
        "# celery beat schedule file": true,
        "celerybeat-schedule": true,
        "# SageMath parsed files": true,
        "*.sage.py": true,
        "# Environments": true,
        ".venv": true,
        "env/": true,
        "ENV/": true,
        "venv/": true,
        "env.bak/": true,
        "venv.bak/": true,
